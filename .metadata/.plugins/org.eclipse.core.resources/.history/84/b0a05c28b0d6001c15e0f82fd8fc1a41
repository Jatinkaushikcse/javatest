package com.kaushik;

public class VolandConsonent {
    
	public static void main(String[] args){
		check('a');
		
	}
	 static void check(char ch) {
		 
		switch(ch) {
		case  'a': 
		case 'e' : 
		case  'i': 
		case  'o':
		case  'u': System.out.println("vowel");
		break;
		default : System.out.println("consonent");
			
		}
	}
}
