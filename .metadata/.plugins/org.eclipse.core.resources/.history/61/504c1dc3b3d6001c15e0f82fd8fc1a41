package com.kaushik;

public class VolandConsonent {
    
	public static void main(String[] args){
		
//		char a = 65;
//		System.out.println(a);
		check('@');
		
	}
	 static void check(char ch) {
		 byte b = (byte)ch;
		 System.out.println(b);
		switch(ch) {
		case  'a': 
		case 'e' : 
		case  'i': 
		case  'o':
		case  'u': 
		case 'A':
		case 'E' : 
		case  'I': 
		case  'O':
		case  'U':	System.out.println("vowel");
		break;
		default : System.out.println("consonent");
			
		}
	}
}
